#git commands
alias gs="git status"
alias gco="git checkout"
alias ga="git add"
alias gb="git branch"
alias gbm="git branch -m"
alias gc="git commit"
alias gcm="git commit -m"
alias getk="gitk 2>/dev/null &"
alias headcarrot='git reset HEAD^'
alias gitcarrot='ga .; git commit -m headcarrot'
alias gfp='git fetch -p'
alias pushhead='git push -u origin'
alias plushead='git push origin +HEAD'
alias grc="git rebase --continue"
alias gra='git rebase --abort'
alias gcp='git cherry-pick'
alias gap='git add -p'
alias giff="git diff"
alias gcoob="git checkout origin/develop -b"
alias gcoom="git checkout origin/master -b"
alias grod="git rebase origin/develop"
alias grom="git rebase origin/master"
alias griod="git rebase -i origin/develop"
alias gamend="gc --amend --no-edit"
alias g-="gco -- "
alias rmds="find . -type f -name '*.DS_Store' -ls -delete"
alias grh='git reset HEAD'
alias gpatch='git apply '
alias dangerousdelete='git branch --merged | grep -v "\*" | xargs -n 1 git branch -d'
alias gubmodule='git submodule update --remote'
alias glame='git blame'
alias glog='git log '
alias gma='git merge --abort'
alias gcpa='git cherry-pick --abort'
